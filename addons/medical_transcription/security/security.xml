<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="module_category_medical_transcription" model="ir.module.category">
        <field name="name">Medical Transcription</field>
        <field name="sequence">50</field>
    </record>

    <record id="group_medical_transcription_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_category_medical_transcription"/>
    </record>

    <record id="group_medical_transcription_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="module_category_medical_transcription"/>
        <field name="implied_ids" eval="[(4, ref('group_medical_transcription_user'))]"/>
    </record>
</odoo>
